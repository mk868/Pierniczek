<catel:DataWindow x:Class="Pierniczek.Views.NewRangeDataWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pierniczek.Views"
        xmlns:models="clr-namespace:Pierniczek.Models"
        mc:Ignorable="d"
        xmlns:core="clr-namespace:System;assembly=mscorlib"
        xmlns:catel="http://schemas.catelproject.com"
        Title="Range" Height="90.572" Width="358.581">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="auto"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="auto"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="auto"/>
        </Grid.ColumnDefinitions>
        <Label Grid.Column="0" Content="&#x3C;" Height="30"/>
        <TextBox Grid.Column="1" Text="{Binding Min}" Height="30" Margin="10,0,10,0"/>
        <Label Grid.Column="2" Content=";" Height="30"/>
        <TextBox Grid.Column="3" Text="{Binding Max}" Height="30" Margin="10,0,10,0"/>
        <Label Grid.Column="4" Content="&#x3E;" Height="30"/>
    </Grid>
</catel:DataWindow>
