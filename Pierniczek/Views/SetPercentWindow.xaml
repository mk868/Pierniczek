<catel:DataWindow x:Class="Pierniczek.Views.SetPercentWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pierniczek.Views"
        xmlns:catel="http://schemas.catelproject.com"
        mc:Ignorable="d"
		Title="Percent" Height="90.572" Width="358.581">
	<Grid>
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="auto"/>
			<ColumnDefinition Width="*"/>
		</Grid.ColumnDefinitions>
		<Label Grid.Column="0" Content="%" Height="30"/>
		<TextBox Grid.Column="1" Text="{Binding Percent}" Height="30" Margin="10,0,10,0"/>
	</Grid>
</catel:DataWindow>
